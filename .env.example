# Pinecone - Required for document indexing and chat
PINECONE_API_KEY=your-pinecone-api-key

# Firecrawl - Required for PDF metadata extraction
FIRECRAWL_API_KEY=your-firecrawl-api-key

# Anthropic - Required for metadata extraction
ANTHROPIC_API_KEY=your-anthropic-api-key

# Convex - Required for database (set automatically by Convex CLI)
CONVEX_DEPLOYMENT=your-convex-deployment
NEXT_PUBLIC_CONVEX_URL=https://your-deployment.convex.cloud

# Clerk - Required for authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key
CLERK_SECRET_KEY=your-clerk-secret-key

# Tinybird - Required for analytics
TINYBIRD_API_URL=https://api.tinybird.co
TINYBIRD_INGEST_TOKEN=your-tinybird-ingest-token
TINYBIRD_ANALYTICS_API_KEY=your-tinybird-analytics-api-key
